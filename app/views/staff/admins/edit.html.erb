<% @page_title = "Edit Admin: #{@admin.full_name}" %>

<%= link_to('&laquo; Back to List'.html_safe, staff_admins_path, :class => 'back-link') %>

<div class="admin edit">
  <h1><%= @page_title %></h1>

  <%= form_for([:staff, @admin]) do |f| %>
    <%= render(:partial => "form", :locals => {:f => f}) %>
    <div id="operations">
      <%= submit_tag "Update Admin" %>
    </div>
  <% end %>
</div>
